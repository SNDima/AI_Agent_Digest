{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug AI Agent Digest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONUNBUFFERED": "1"
            },
            "envFile": "${workspaceFolder}/.env",
            "stopOnEntry": false,
            "args": []
        },
        {
            "name": "Debug Search Agent",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/search/agent.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONUNBUFFERED": "1"
            },
            "envFile": "${workspaceFolder}/.env",
            "stopOnEntry": false,
            "args": []
        }
    ]
}
